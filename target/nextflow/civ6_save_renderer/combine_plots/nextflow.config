  docker.enabled = true
  docker.runOptions = "-i -v ${baseDir}:${baseDir}"
  process.container = "dataintuitive/portash"
  params {
    combine_plots__input = "/path/to/my/dir"
    combine_plots__output = "output.webm"
    combine_plots__framerate = "4"
    id = ""
    dockerPrefix = ""
    input = ""
    output = ""
    testScript = ""
    testResources = [  ]
    combine_plots {
      name = "combine_plots"
      container = "jrottenberg/ffmpeg"
      containerTag = "latest"
      command = "combine_plots"
      tests {
        isDefined = false
        testScript = "NA"
        testResources = [  ]
      }
      extensions {
        out = "webm"
      }
      arguments {
        input {
          name = "input"
          otype = "--"
          required = true
          type = "file"
          direction = "Input"
          multiple = true
          multiple_sep = ":"
          description = "A list of images."
          value = "${params.combine_plots__input}"
        }
        output {
          name = "output"
          otype = "--"
          required = true
          type = "file"
          direction = "Output"
          multiple = false
          multiple_sep = ":"
          description = "A path to output the movie to."
          value = "${params.combine_plots__output}"
        }
        framerate {
          name = "framerate"
          otype = "--"
          required = false
          type = "integer"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          description = "Number of frames per second."
          value = "${params.combine_plots__framerate}"
        }
      }
    }
  }